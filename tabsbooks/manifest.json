{
  "name": "Tabs and bookmarks",
  "description": "Export Tabs and Bookmarks",
  "version": "1.0",
  "permissions": [
	"bookmarks",
    "tabs", 
	"http://*/*", 
	"https://*/*"
  ],
  "browser_action": {
      "default_title": "TabsBookmarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
